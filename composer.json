{
    "name": "thelia/thelia",
    "description": "Thelia is an ecommerce CMS.",
    "license": "LGPL-3.0+",
    "homepage": "http://thelia.net/",
    "support": {
        "forum": "http://thelia.net/forum",
        "wiki": "http://doc.thelia.net"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "fzaninotto/faker": "1.5.x-dev",
        "thelia/hooktest-module": "~1.0",
        "thelia/hooktest-template": "~1.0"
    },
    "minimum-stability": "stable",
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "core/vendor"
    },
    "includes": {
          "core/lib/Thelia/composer.json": []
    },
    "autoload": {
        "psr-0": {
            "": "local/modules/",
            "Thelia": "core/lib/",
            "Thelia\\Tests" : "tests/phpunit/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.0-dev"
        }
    }
}
